{
  "name": "ember-cli-barcode",
  "version": "0.1.5",
  "description": "Ember barcode generator using the JSBarcode library.",
  "keywords": [
    "ember-addon",
    "barcode",
    "jsbarcode"
  ],
  "license": "MIT",
  "author": "Max Wondercorn aka Gregg Martell",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maxwondercorn/ember-cli-barcode"
  },
  "ember-addon": {
    "demoURL": "https://maxwondercorn.github.io/ember-cli-barcode/",
    "configPath": "tests/dummy/config"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember serve",
    "test": "ember try:each",
    "deploy": "ember build && ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages"
  },
  "dependencies": {
    "ember-cli-babel": "^6.12.0",
    "ember-cli-htmlbars": "^2.0.3"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.7.0",
    "ember-ajax": "^3.1.0",
    "ember-cli": "^2.18.2",
    "ember-cli-bootstrap-sassy": "^0.5.8",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-github-pages": "^0.1.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-sass": "^7.2.0",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.5.5",
    "ember-source": "^2.18.2",
    "loader.js": "^4.7.0"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  }
}
